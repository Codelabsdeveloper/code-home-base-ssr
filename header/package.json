{
  "name": "header",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build -p 3001",
    "start": "next start -p 3001",
    "es-lint": "eslint . --ext .jsx,.js",
    "es-lint:fix": "eslint . --ext .jsx,.js --fix",
    "prettier": "prettier  --check \"**/*.{jsx,js,json}\"",
    "prettier:fix": "prettier --check --write \"**/*.{jsx,js,json}\" ",
    "lint": "npm run es-lint && npm run prettier",
    "lint:fix": "npm run es-lint:fix && npm run prettier:fix",
    "pre-push:validation": "npm run lint && npm run build"
  },
  "pre-commit": [
    "lint"
  ],
  "pre-push": [
    "pre-push:validation"
  ],
  "dependencies": {
    "@module-federation/nextjs-mf": "^1.1.1",
    "next": "10.2.3",
    "node-sass": "^7.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "webpack": "5.38.1"
  },
  "devDependencies": {
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "postcss": "^8.4.16",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.7.2",
    "postcss-pxtorem": "^6.0.0",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.3",
    "prettier": "^2.7.1",
    "sass": "^1.54.4",
    "sass-loader": "^13.0.2"
  }
}
